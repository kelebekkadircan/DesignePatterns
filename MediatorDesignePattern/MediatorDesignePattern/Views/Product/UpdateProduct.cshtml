@model MediatorDesignePattern.MediatorPattern.Results.UpdateProductByIDQueryResult;



@{
    Layout = null;
}

 <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

 <div class="container" >

<form method="post">
        <br/>
        <label>Ürün ID</label>
        <input class="form-control" type="text" asp-for="ProductID" /> <br/>
        <label>Ürün Adı</label>
        <input class="form-control" type="text" asp-for="ProductName" /> <br/>
        <label>Ürün Fiyatı</label>
        <input class="form-control" type="text" asp-for="ProductPrice" /> <br />
        <label>Stok Türü</label>
        <input class="form-control" type="text" asp-for="ProductStockType" /> <br />
        <label>Stok Sayısı</label>
        <input class="form-control" type="text" asp-for="ProductStock" /> <br />
        <label>Ürün Kategori</label>
        <input class="form-control" type="text" asp-for="ProductCategory" /> <br />
        <button class="btn btn-outline-primary" >Güncelle</button>

</form>

 </div>

